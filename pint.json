{
    "preset": "laravel",
    "rules": {
        "no_superfluous_phpdoc_tags": false,
        "phpdoc_align": false,
        "phpdoc_separation": false,
        "single_quote": false,
        "not_operator_with_successor_space": false,
        "concat_space": false,
        "no_extra_blank_lines": false,
        "declare_strict_types": false,
        "no_unused_imports": false
    }
}


